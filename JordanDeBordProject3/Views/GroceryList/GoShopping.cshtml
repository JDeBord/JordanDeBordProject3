@model JordanDeBordProject3.Models.ViewModels.GoShoppingListVM


<h1>@Model.Name</h1>

<table id="permission-@Model.GroceryListUserId"class="table">
    <tbody>
        @foreach (var item in Model.GroceryItems)
        {
            <partial name="_AddShoppingItem" model="@item" />
        }
    </tbody>
</table>

<a class="btn btn-primary" asp-controller="Home" asp-action="Index">Back to Home</a>

@section Scripts{
    <script src="~/js/groceryListGoShopping.js" defer></script>
}